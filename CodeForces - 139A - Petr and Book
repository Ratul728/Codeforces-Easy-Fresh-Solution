#include<bits/stdc++.h>

using namespace std;


int main()
{
    int t,i;
    int a[8];

    cin>>t;

    for(int i = 1; i < 8; i++)
        cin>>a[i];

    i = 1;

    while(t > 0)
    {

        t = t - a[i];

        i++;

        if(i == 8)
            i = 1;

    }

    if(i == 1)
        cout<<"7"<<endl;
    else
        cout<<(i - 1)<<endl;



    return 0;

}
