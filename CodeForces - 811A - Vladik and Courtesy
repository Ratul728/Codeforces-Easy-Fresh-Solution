#include<bits/stdc++.h>

using namespace std;

int main()
{
  int a,b, m = 1, n = 2;

  cin>>a>>b;

  for(int i = 1; ; i++)
  {
      a -= i;

      if(a < 0)
      {
          cout<<"Vladik"<<endl;
          break;
      }

      i++;

      b -= i;

      if(b < 0)
      {
          cout<<"Valera"<<endl;
          break;
      }
  }
}
