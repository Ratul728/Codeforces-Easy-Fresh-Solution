#include<bits/stdc++.h>

using namespace std;

int main()
{
    vector <int> v;

    int a,b,c,x1,x2,x3,x4, x, m = -1;

    for(int i = 0; i < 4; i++)
    {
        cin>>x1;

        v.push_back(x1);
    }

    sort(v.begin(), v.end());

    b = ((v[0] - v[1]) + v[2]) / 2;

    a = v[0] - b;

    c = v[2] - b;

    cout<<a<<" "<<b<<" "<<c<<endl;

    v.clear();
}
